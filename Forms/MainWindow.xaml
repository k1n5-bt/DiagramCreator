<Window x:Class="DiagramCreator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:diagramCreater="clr-namespace:DiagramCreator"
        mc:Ignorable="d">
    <Grid>
        <WrapPanel Orientation="Vertical">
            <Button Click="Add" Content="Add" Height="30" Width="60" />
            <Button Click ="DeleteAllFigures" Content="Delete All" Height="30" Width="60" />
            <Button Click="Save" Content="Save" Height="30" Width="60" />
        </WrapPanel>
        <diagramCreater:Control x:Name="Warp">
        </diagramCreater:Control>
    </Grid>
</Window>